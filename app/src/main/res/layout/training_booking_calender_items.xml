<?xml version="1.0" encoding="utf-8"?>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >



    <View
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x1"
        android:alpha="0.6"
        android:background="@drawable/text"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="10">

        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtDayDate"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginRight="@dimen/x15"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="2"
            android:gravity="center|center_vertical|center_horizontal"
            android:text="Fri 7"
            app:typeface="Roboto-Regular.ttf"

            />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:alpha="0.6"
            android:background="@drawable/text" />


        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtback"
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x10"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="0.5"
            android:background="@drawable/back"
            android:gravity="center|center_vertical|center_horizontal"
            android:visibility="gone"
            app:typeface="Roboto-Regular.ttf"

            />
        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtback1"
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x10"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="0.5"
            android:background="@drawable/back"
            android:gravity="center|center_vertical|center_horizontal"
            android:visibility="gone"
            app:typeface="Roboto-Regular.ttf"

            />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:alpha="0.6"
            android:background="@drawable/text" />


        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtSecTo"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginRight="@dimen/x15"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="2"
            android:background="@drawable/background_day_hover"
            android:gravity="center|center_vertical|center_horizontal"
            android:text=""
            android:visibility="gone"
            app:typeface="Roboto-Regular.ttf"

            />

        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtTo1"
            android:layout_width="@dimen/x75"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginRight="@dimen/x15"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="2"
            android:background="@drawable/background_day_hover"
            android:gravity="center|center_vertical|center_horizontal"
            android:text=""
            app:typeface="Roboto-Regular.ttf"

            />

        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtThirdTo"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginRight="@dimen/x15"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="2"
            android:background="@drawable/background_day_hover"
            android:gravity="center|center_vertical|center_horizontal"
            android:text=""
            android:visibility="gone"
            app:typeface="Roboto-Regular.ttf"

            />

        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtcenter1"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x5"
            android:layout_marginRight="@dimen/x5"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="4"
            android:gravity="center|center_vertical|center_horizontal"
            android:text="to"
            app:typeface="Roboto-Regular.ttf"

            />

        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtFrom1"
            android:layout_width="@dimen/x75"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginRight="@dimen/x15"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="2"
            android:background="@drawable/background_day_hover"
            android:gravity="center|center_vertical|center_horizontal"
            android:text=""
            app:typeface="Roboto-Regular.ttf"


            />

        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtSecFrom"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginRight="@dimen/x15"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="2"
            android:background="@drawable/background_day_hover"
            android:gravity="center|center_vertical|center_horizontal"
            android:text=""
            android:visibility="gone"
            app:typeface="Roboto-Regular.ttf"

            />


        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtThirdFrom"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x15"
            android:layout_marginRight="@dimen/x15"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="2"
            android:background="@drawable/background_day_hover"
            android:gravity="center|center_vertical|center_horizontal"
            android:text=""
            android:visibility="gone"
            app:typeface="Roboto-Regular.ttf"

            />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal"
            android:alpha="0.6"
            android:background="@drawable/text" />

        <com.app.ace.ui.views.AnyTextView
            android:id="@+id/txtIncre1"
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/x10"
            android:layout_marginLeft="@dimen/x10"
            android:layout_marginRight="@dimen/x10"
            android:layout_marginTop="@dimen/x10"
            android:layout_weight="0.5"
            android:background="@drawable/addinc"
            android:gravity="center|center_vertical|center_horizontal"
            app:typeface="Roboto-Regular.ttf" />



    </LinearLayout>


    <View
        android:layout_width="wrap_content"
        android:layout_height="0.5dp"
        android:alpha="0.6"
        android:background="@drawable/text"/>

</LinearLayout>